{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "noResolve": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "strictNullChecks": false,
    "allowJs": true,
    "pretty": true,
    "outDir": "./js_server",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": [
    "./src/**/*"
  ]
}